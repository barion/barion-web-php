{
  "name": "snufkin/barion-web-php",
  "description": "Barion PHP Library",
  "keywords": [
    "barion",
    "payment processing",
    "api"
  ],
  "homepage": "https://www.barion.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Barion and contributors",
      "homepage": "https://github.com/barion/barion-web-php"
    }
  ],
  "require": {
    "php": ">=5.3.3",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "squizlabs/php_codesniffer": "~2.0"
  },
  "autoload": {
    "psr-4": { "Barion\\" : "lib/" }
  },
  "extra": {
    "branch-alias": {
      "master": "1.0-dev"
    }
  }
}
